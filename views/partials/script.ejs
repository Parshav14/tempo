<% if(typeof success !== 'undefined' && success.length > 0){ %>
    <script>
      toastr.success('<%=success%>')
    </script>
<% } %>

<% if(typeof errors !== 'undefined' && errors.length > 0){ %>
    <script>
      toastr.error('<%=errors%>')
    </script>
<% } %>

<% if(typeof scriptFile !== 'undefined' && scriptFile){ %>
    <%- scriptFile %>
<% } %>